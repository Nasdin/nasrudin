# Physics Generator - Local Development Environment
# Copy this file to .env and fill in your values:
#   cp .env.example .env

# ── PostgreSQL ─────────────────────────────────────────────
POSTGRES_USER=physics
POSTGRES_PASSWORD=physics_dev
POSTGRES_DB=physics_generator
POSTGRES_PORT=5432

# Connection string used by Rust engine (SeaORM)
DATABASE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@localhost:${POSTGRES_PORT}/${POSTGRES_DB}

# ── Rust Engine ────────────────────────────────────────────
RUST_LOG=info
API_PORT=3001
ROCKS_DB_PATH=./data/theorems.db

# ── Frontend ───────────────────────────────────────────────
VITE_API_URL=http://localhost:${API_PORT}

